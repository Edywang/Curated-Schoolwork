@#!/bin/sh
# $Id: test.run,v 1.3 2020-11-26 18:22:16-08 - - $

function testrun {
   echo
   echo ======== $*
   run.perl $*; echo status = $?
}

cid -si $0 run.perl
testrun
testrun error 0
testrun error 1
testrun error -1
testrun abort
testrun segfault
testrun zerodivide
testrun foo bar baz
@


